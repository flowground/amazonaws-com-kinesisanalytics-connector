{
    "type": "object",
    "properties": {
        "Action": {
            "required": true,
            "type": "string"
        },
        "Version": {
            "required": true,
            "type": "string"
        },
        "requestBody": {
            "properties": {
                "ApplicationName": {
                    "description": "The Kinesis Analytics application name.",
                    "maxLength": 128,
                    "minLength": 1,
                    "pattern": "[a-zA-Z0-9_.-]+",
                    "type": "string"
                },
                "CurrentApplicationVersionId": {
                    "description": "The version ID of the Kinesis Analytics application.",
                    "maximum": 999999999,
                    "minimum": 1,
                    "type": "integer"
                },
                "InputId": {
                    "description": "The ID of the input configuration from which to delete the input processing configuration. You can get a list of the input IDs for an application by using the <a>DescribeApplication</a> operation.",
                    "maxLength": 50,
                    "minLength": 1,
                    "pattern": "[a-zA-Z0-9_.-]+",
                    "type": "string"
                }
            },
            "required": [
                "ApplicationName",
                "CurrentApplicationVersionId",
                "InputId"
            ],
            "type": "object"
        },
        "X_Amz_Content_Sha256": {
            "required": false,
            "type": "string"
        },
        "X_Amz_Date": {
            "required": false,
            "type": "string"
        },
        "X_Amz_Algorithm": {
            "required": false,
            "type": "string"
        },
        "X_Amz_Credential": {
            "required": false,
            "type": "string"
        },
        "X_Amz_Security_Token": {
            "required": false,
            "type": "string"
        },
        "X_Amz_Signature": {
            "required": false,
            "type": "string"
        },
        "X_Amz_SignedHeaders": {
            "required": false,
            "type": "string"
        }
    }
